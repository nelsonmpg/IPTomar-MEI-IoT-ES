<p id="notice"><%= notice %></p>

<h1>Tags</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Resume</th>
      <th>Date</th>
      <th>github</th>
      <th>grade</th>
      <th>project_url</th>
      <th>presentation</th>
      <th>featured</th>
      <th>finished</th>
      <th>doc</th>

      <th>User</th>
      <th>Discipline</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.resume %></td>
        <td><%= project.date %></td>
        <td><%= project.github %></td>
        <td><%= project.grade %></td>
        <td><%= project.project_url %></td>
        <td><%= project.presentation %></td>
        <td><%= project.featured %></td>
        <td><%= project.finished %></td>
        
        <td><%= project.user_id %></td>
        <td><%= project.subject_id %></td>
        <td><%= project.titledoc %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', projects_path %>
<P> ---- User ---- </p>
<div id="user_nav">
  <% if user_signed_in? %>
    Signed in as : <%= current_user.email %>. Not you?
    <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
  <% else %>
    <%= link_to "Sign up", new_user_registration_path %> or <%= link_to "sign in", new_user_session_path %>
  <% end %>
</div>
