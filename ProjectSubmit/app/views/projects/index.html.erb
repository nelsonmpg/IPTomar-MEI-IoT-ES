<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Resume</th>
      <th>Date</th>
      <th>github</th>
      <th>grade</th>
      <th>projectlink</th>
      <th>presentation</th>
      <th>feature</th>
      <th>finished</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.resume %></td>
        <td><%= project.date %></td>
        <td><%= project.github %></td>
        <td><%= project.grade %></td>
        <td><%= project.projectlink %></td>
        <td><%= project.presentation %></td>
        <td><%= project.feature %></td>
        <td><%= project.finished %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
